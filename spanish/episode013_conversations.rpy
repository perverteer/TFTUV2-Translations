    # e "I think they will."
    e "Creo que lo harán."

# game/episode013_conversations.rpy:599
translate spanish ep013_eva_talk_b4243383:

    # e "Lilly spent almost all of her time by your side."
    e "Lilly pasó casi todo su tiempo a tu lado."

# game/episode013_conversations.rpy:601
translate spanish ep013_eva_talk_fd4d4f67:

    # e "Aven spent almost all of her time by your side."
    e "Aven pasó casi todo su tiempo a tu lado."

# game/episode013_conversations.rpy:602
