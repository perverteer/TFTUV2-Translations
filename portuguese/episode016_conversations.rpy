translate portuguese ep016_thyia_talk_f5e55463:

    # c "Wedding first maybe?"
    c "Talvez um casamento antes disso?"

# game/episode016_conversations.rpy:548
translate portuguese ep016_thyia_talk_7daf669d:

    # c "You in that white princess dress?"
    c "Você naquele vestido branco de princesa?"

