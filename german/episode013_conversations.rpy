    # c "You're jealous of Céline?"
    c "Bist du eifersüchtig auf Céline?"

# game/episode013_conversations.rpy:464
translate german ep013_lilly_second_talk_b3033db7:

    # l "That's what I said, didn't I?"
    l "Genau das habe ich doch gesagt, oder nicht?"

# game/episode013_conversations.rpy:466
translate german ep013_lilly_second_talk_aea652cc:

    # l "She was walking her stupid little walk, her arrogant head way up in the air and then she dropped our supplies."
    l "Sie lief ihren albernen kleinen Gang entlang, den Kopf hoch erhoben, und dann ließ sie unsere Vorräte fallen."

# game/episode013_conversations.rpy:467
translate german ep013_lilly_second_talk_8aed8e3c:

    # c "Lilly, she's one of your best friends."
    c "Lilly, sie ist eine eurer besten Freundinnen."

# game/episode013_conversations.rpy:468
translate german ep013_lilly_second_talk_78fd3f93:

    # l "I don't care."
    l "Das ist mir egal."

# game/episode013_conversations.rpy:469
translate german ep013_lilly_second_talk_7a4d87a1:

    # l "She has her filthy little claws all over you."
    l "Sie hat ihre schmutzigen kleinen Krallen überall an dir."

# game/episode013_conversations.rpy:471
translate german ep013_lilly_second_talk_f1bb613b:

    # c "Lilly, you know I’ve broken up with her."
    c "Lilly, ihr wisst, dass ich mit ihr Schluss gemacht habe."

# game/episode013_conversations.rpy:472
