    # l "Yes...{w} No."
    l "Ja...{w} Nein."

# game/episode013_conversations.rpy:449
translate german ep013_lilly_second_talk_71887d92:

    # c "What the hell happened?"
    c "Was zum Teufel ist passiert?"

# game/episode013_conversations.rpy:451
