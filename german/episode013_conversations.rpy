    # ce "Jealousy?"
    ce "Eifersucht?"

# game/episode013_conversations.rpy:949
translate german ep013_celine_second_talk_12d2e699:

    # ce "So why is she jealous?"
    ce "Warum ist sie denn eifersüchtig?"

# game/episode013_conversations.rpy:950
translate german ep013_celine_second_talk_c9fc2a0b:

    # ce "Does she think we aren't a good match?"
    ce "Glaubt sie, dass wir nicht gut zusammenpassen?"

# game/episode013_conversations.rpy:951
translate german ep013_celine_second_talk_98b9b44f:

    # ce "I've never known her to be so possessive."
    ce "Ich habe sie noch nie so besitzergreifend erlebt."

# game/episode013_conversations.rpy:952
translate german ep013_celine_second_talk_b7ecd7cf:

    # c "No, it's not that."
    c "Nein, das ist es nicht."

# game/episode013_conversations.rpy:954
translate german ep013_celine_second_talk_8e3fe1f5:

    # ce "What's the reason then?"
    ce "Was ist denn dann der Grund?"

# game/episode013_conversations.rpy:955
translate german ep013_celine_second_talk_82174601:

    # ce "..."
    ce "..."

# game/episode013_conversations.rpy:957
translate german ep013_celine_second_talk_cc189b99:

    # ce "Oh...{w} No, that can't be it."
    ce "Oh...{w} Nein, das kann es nicht sein."

# game/episode013_conversations.rpy:958
translate german ep013_celine_second_talk_b8b60340:

    # ce "Is Lilly in love with you?"
    ce "Ist Lilly in dich verliebt?"

# game/episode013_conversations.rpy:960
translate german ep013_celine_second_talk_1bec0e5b:

