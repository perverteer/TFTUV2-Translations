    # l "Must we talk about this?"
    l "Müssen wir darüber reden?"

# game/episode013_conversations.rpy:452
translate german ep013_lilly_second_talk_be98474e:

    # c "Yes, we must."
    c "Ja, das müssen wir."

# game/episode013_conversations.rpy:453
translate german ep013_lilly_second_talk_b6bd30a5:

    # c "Why did you go off at Céline like that?"
    c "Warum bist du so zu Céline ausgerastet?"

# game/episode013_conversations.rpy:455
translate german ep013_lilly_second_talk_aad1d645:

    # l "Well, isn't it obvious?"
    l "Nun, ist das nicht offensichtlich?"

# game/episode013_conversations.rpy:456
translate german ep013_lilly_second_talk_5a526a85:

    # c "I can guess, but I'd like to hear it from you."
