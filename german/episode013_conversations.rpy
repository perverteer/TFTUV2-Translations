    # e "Oh yes, maybe Nadya and I would have worked together."
    e "Oh ja, vielleicht hätten Nadya und ich zusammengearbeitet."

# game/episode013_conversations.rpy:583
translate german ep013_eva_talk_03f88cfa:

    # c "The mother and daughter team of star scientists."
    c "Das Mutter-Tochter-Team aus Spitzenwissenschaftlerinnen."

# game/episode013_conversations.rpy:585
translate german ep013_eva_talk_584e9b9d:

    # e "That’s right!"
    e "Das stimmt!"

# game/episode013_conversations.rpy:586
translate german ep013_eva_talk_c6bf516f:

    # c "Do you know how Lilly is taking all of this?"
    c "Weißt du, wie Lilly das alles aufnimmt?"

