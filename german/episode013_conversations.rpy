translate german ep013_lilly_second_talk_1a16c951:

    # c "If possible, yes."
    c "Wenn möglich, ja."

# game/episode013_conversations.rpy:517
translate german ep013_lilly_second_talk_7e6c60a0:

    # l "Alright, fine."
    l "Na gut, in Ordnung."

