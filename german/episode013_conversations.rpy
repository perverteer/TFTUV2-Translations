    # l "So?!"
    l "Also?!"

# game/episode013_conversations.rpy:473
translate german ep013_lilly_second_talk_ac9785bf:

    # c "She and I are done.{w} History."
    c "Sie und ich sind fertig.{w} Verlauf."

# game/episode013_conversations.rpy:475
translate german ep013_lilly_second_talk_0a62f9c1:

    # c "Lilly..."
    c "Lilly..."

# game/episode013_conversations.rpy:477
