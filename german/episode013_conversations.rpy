    # l "It was jealousy, okay."
    l "Es war Eifersucht, okay?"

# game/episode013_conversations.rpy:461
translate german ep013_lilly_second_talk_ebea651f:

    # l "Happy now?"
    l "Jetzt zufrieden?"

# game/episode013_conversations.rpy:462
