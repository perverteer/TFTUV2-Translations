    # ce "I really don't know what got into her, she's never yelled at me like that."
    ce "Ich weiß wirklich nicht, was in sie gefahren ist, sie hat mich noch nie so angeschrien."

# game/episode013_conversations.rpy:938
translate german ep013_celine_second_talk_4387e8d3:

    # c "I told Lilly everything about you and me."
    c "Ich habe Lilly alles über dich und mich erzählt."

# game/episode013_conversations.rpy:940
translate german ep013_celine_second_talk_3bdd7b97:

    # ce "Oh?"
    ce "Oh?"

# game/episode013_conversations.rpy:941
translate german ep013_celine_second_talk_860fe87c:

