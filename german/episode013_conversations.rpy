translate german ep013_eva_talk_779dd43d:

    # e "Well...{w} Not great."
    e "Nun ja...{w} Nicht so toll."

# game/episode013_conversations.rpy:590
translate german ep013_eva_talk_16d38471:

    # e "You know how she can be."
    e "Du weißt ja, wie sie sein kann."

# game/episode013_conversations.rpy:592
