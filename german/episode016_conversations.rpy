# game/episode016_conversations.rpy:128
translate german ep016_eva_talk_730bdc9a:

    # e "I mean, I thought I’d lost you just a short while back!"
    e "Ich meine, ich dachte, ich hätte dich vor Kurzem schon verloren!"

# game/episode016_conversations.rpy:130
translate german ep016_eva_talk_ed5801e6:

    # e "And then...{w} We kissed and it triggered all these confusing feelings I’ve felt for you for so long."
    e "Und dann...{w} haben wir uns geküsst und das hat all diese verwirrenden Gefühle ausgelöst, die ich schon so lange für dich empfinde."

# game/episode016_conversations.rpy:131
translate german ep016_eva_talk_c5255654:

    # e "It’s just..."
    e "Es ist einfach..."

# game/episode016_conversations.rpy:133
translate german ep016_eva_talk_cc197c6b:

