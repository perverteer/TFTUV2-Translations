# game/episode019.rpy:2105
translate german ep019_paclerara_jade_926a80e6:

    # c "But I am, Jade!"
    c "Aber ich bin es doch, Jade!"

# game/episode019.rpy:2106
translate german ep019_paclerara_jade_397abb57:

    # c "As I said, you’d be a great mother."
